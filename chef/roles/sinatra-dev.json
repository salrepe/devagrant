{
  "json_class": "Chef::Role",
  "name": "sinatra-dev",
  "description": "Sinatra development",
  "run_list": [
    "recipe[git]",
    "recipe[vim]",
    "recipe[postgresql]",
    "recipe[ruby_build]",
    "recipe[rbenv::user]"
  ],
  "override_attributes": {
    "postgresql": {
      "password": {
        "postgres": ""
      }
    },
    "rbenv": {
      "user_installs": [
        {
          "user": "vagrant",
          "rubies": ["2.1.1"],
          "global": "2.1.1",
          "gems": {
            "2.1.1": [
              { "name": "bundler" }
            ]
          }
        }
      ]
    }
  }
}
